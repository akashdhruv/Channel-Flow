# Configuration file for `jobrunner`

# schedular submit command,
# a simple `sbatch` or `bsub` or `qsub`
schedular.command = "sbatch"

schedular.options = [
    "#SBATCH --ntasks=4",
    "#SBATCH --time=0-00:30",
    "#SBATCH --job-name=vel_animation",
]

job.submit = ["run.sh"]

